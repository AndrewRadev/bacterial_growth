#! /usr/bin/env python3

import os
from db import get_config

config = get_config()

os.execv(
    '/usr/bin/mysql', [
        '/usr/bin/mysql',
        '--local-infile',
        f'-h{config["host"]}',
        f'-u{config["username"]}',
        f'-p{config["password"]}',
        config['database']
    ]
)
