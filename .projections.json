{
  "templates/*.html": {
    "type": "template"
  },
  "forms/*.py": {
    "type": "form"
  },
  "models/*.py": {
    "type": "model",
    "alternate": "tests/models/test_{}.py"
  },
  "pages/*.py": {
    "type": "page"
  },
  "static/css/*.css": {
    "type": "stylesheet"
  },
  "static/js/*.js": {
    "type": "javascript"
  },
  "tests/**/test_*.py": {
    "type": "test",
    "alternate": "{}.py"
  }
}
